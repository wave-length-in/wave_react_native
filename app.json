{
  "expo": {
    "name": "Wave.ai",
    "slug": "wave-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "scheme": "waveapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UILaunchScreen": {
          "UIImageName": "SplashScreen",
          "UILaunchScreenMinimumSize": "{320, 568}"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "splash": {
        "image": "./assets/images/wave-new-logo2.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000",
        "mdpi": "./assets/images/app_icon.png",
        "hdpi": "./assets/images/app_icon.png",
        "xhdpi": "./assets/images/app_icon.png",
        "xxhdpi": "./assets/images/app_icon.png",
        "xxxhdpi": "./assets/images/app_icon.png"
      },
      "package": "com.gauravwavedev.waveapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/wave-new-logo2.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/wave-new-logo2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "image": "./assets/images/wave-new-logo2.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/images/wave-new-logo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "extra": {
      "eas": {
        "projectId": "2e186c1e-13e7-41f9-aa5c-d90840c55f1a"
      }
    }
  }
}